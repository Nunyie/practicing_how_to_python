#Nunyie
#Prints all numbers once even if inputted more than once

#defining valid inputs
def valid_input(u):
    while True:
        try:
            return float(input(u))
        except ValueError:
            print("Please enter a number")

numbers = []
for i in range(10):
    num = valid_input(f"Enter Number {i + 1}: ")
    numbers.append(num)
